previous_annual_family_income = execution.getVariable("previous_annual_family_income")
csgd_income_cap = execution.getVariable("csgd_income_cap")
if(csgd_amount &amp;&amp; csgd_weekly_phaseout_slope &amp;&amp; previous_annual_family_income &amp;&amp; csgd_income_cap) {
  execution.getVariable("csgd_eligibility") == "yes" ?  Math.min(Math.max(0, csgd_amount - csgd_weekly_phaseout_slope * ( previous_annual_family_income - csgd_income_cap)) , execution.getVariable("csgd_amount")) : 0;
}</camunda:script>
