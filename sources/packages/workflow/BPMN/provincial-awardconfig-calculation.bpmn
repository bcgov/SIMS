          <camunda:outputParameter name="net_csgft_amount">
            <camunda:script scriptFormat="groovy">execution.getVariable("csgft_eligibility") == "yes" ? (execution.getVariable("csgft_grant") &lt; 0 ? 0 : Math.max(100,Math.round(execution.getVariable("csgft_grant")))): 0;</camunda:script>
