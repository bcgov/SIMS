          <camunda:outputParameter name="bcag_weekly_prorated_phaseout_rate">${(bcag_phaseout_slope/100)*(1/8)*12*(1/52)}</camunda:outputParameter>
          <camunda:outputParameter name="bcag_weekly_grant">
            <camunda:script scriptFormat="groovy">bcag_weekly_prorated_max_grant = execution.getVariable("bcag_weekly_prorated_max_grant")
bcag_weekly_prorated_phaseout_rate = execution.getVariable("bcag_weekly_prorated_phaseout_rate")
previous_annual_family_income = execution.getVariable("previous_annual_family_income")
bcag_allowable_max_income = execution.getVariable("bcag_allowable_max_income")
if (bcag_weekly_prorated_max_grant &amp;&amp; bcag_weekly_prorated_phaseout_rate &amp;&amp; previous_annual_family_income &amp;&amp; bcag_allowable_max_income) {
  Math.min(bcag_weekly_prorated_max_grant ,bcag_weekly_prorated_max_grant - (bcag_weekly_prorated_phaseout_rate * (previous_annual_family_income - bcag_allowable_max_income)));
}</camunda:script>
