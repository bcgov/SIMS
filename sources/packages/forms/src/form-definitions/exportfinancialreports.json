              "input": true,
              "inputType": "checkbox",
              "autofocus": false,
              "inline": false,
              "protected": false,
              "persistent": true,
              "hidden": false,
              "clearOnHide": true,
              "labelPosition": "top",
              "conditional": {
                "show": "",
                "when": null,
                "eq": ""
              },
              "properties": {}
            },
            {
              "input": true,
              "tableView": true,
              "label": "SABC Program Code",
              "key": "sabcProgramCode",
              "validate": {
                "maxLength": 4,
                "customMessage": "Maximum 4 characters is allowed."
              },
              "type": "textfield",
              "lockKey": true,
              "customConditional": "show = \n    data.reportName === 'Program_And_Offering_Status_Report'\n    || data.reportName === 'Ministry_Student_Unmet_Need_Report'",
              "autofocus": false,
              "inputType": "text",
