                  "width": 6,
                  "offset": 0,
                  "push": 0,
                  "pull": 0,
                  "size": "md",
                  "currentWidth": 6
                },
                {
                  "components": [
                    {
                      "label": "End date",
                      "placeholder": "Select date",
                      "widget": {
                        "type": "calendar",
                        "allowInput": true,
                        "clickOpens": true,
                        "enableDate": true,
                        "enableTime": false,
                        "mode": "single",
                        "noCalendar": false,
                        "format": "yyyy-MM-dd",
                        "dateFormat": "yyyy-MM-dd",
                        "useLocaleSettings": false,
                        "hourIncrement": 1,
                        "minuteIncrement": 5,
                        "time_24hr": false,
                        "saveAs": "text",
                        "locale": "en",
                        "displayInTimezone": "viewer"
                      },
                      "tableView": true,
                      "validate": {
                        "required": true,
                        "custom": "valid = data.params.startDate\r\n  ? data.params.startDate < data.params.endDate\r\n    ? true\r\n    : \"End Date must be after Start Date\"\r\n  : \"Please provide Start date\";"
                      },
                      "key": "endDate",
                      "attributes": {
                        "data-cy": "endDate"
                      },
                      "type": "textfield",
                      "input": true,
                      "lockKey": true,
                      "autofocus": false,
                      "inputType": "text",
                      "inputMask": "",
                      "prefix": "",
                      "suffix": "",
                      "multiple": false,
                      "defaultValue": "",
                      "protected": false,
                      "unique": false,
                      "persistent": true,
                      "hidden": false,
                      "clearOnHide": true,
                      "spellcheck": true,
                      "conditional": {
                        "show": null,
                        "when": null,
                        "eq": ""
                      }
                    }
                  ],
                  "width": 6,
                  "offset": 0,
                  "push": 0,
                  "pull": 0,
                  "size": "md",
                  "currentWidth": 6
                }
              ],
              "key": "columns",
              "customConditional": "show = \n    data.reportName === 'Disbursement_Forecast_Report' \n    || data.reportName === 'Disbursement_Report' \n    || data.reportName === 'Data_Inventory_Report'\n    || data.reportName === 'Institution_Designation_Report'\n    || data.reportName === 'ECert_Errors_Report'\n    || data.reportName === 'Program_And_Offering_Status_Report'\n    || data.reportName === 'Ministry_Student_Unmet_Need_Report'",
