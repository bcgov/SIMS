                {
                  "name": "Required Validator",
                  "trigger": {
                    "type": "simple",
                    "simple": {
                      "show": true,
                      "when": "reportName",
                      "eq": "Program_And_Offering_Status_Report"
                    }
                  },
                  "actions": [
                    {
                      "name": "Set as required",
                      "type": "property",
                      "property": {
                        "label": "Required",
                        "value": "validate.required",
                        "type": "boolean"
                      },
                      "state": true
                    }
                  ]
                }
              ],
              "type": "select",
              "input": true,
              "lockKey": true,
              "isNew": false,
              "autofocus": false,
              "defaultValue": "",
