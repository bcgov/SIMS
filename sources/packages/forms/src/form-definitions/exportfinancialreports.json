                        "type": "calendar",
                        "allowInput": true,
                        "clickOpens": true,
                        "enableDate": true,
                        "enableTime": false,
                        "mode": "single",
                        "noCalendar": false,
                        "format": "yyyy-MM-dd",
                        "dateFormat": "yyyy-MM-dd",
                        "useLocaleSettings": false,
                        "hourIncrement": 1,
                        "minuteIncrement": 5,
                        "time_24hr": false,
                        "saveAs": "text",
                        "locale": "en",
                        "displayInTimezone": "viewer"
