          "label": "Report Type",
          "widget": "choicesjs",
          "tableView": true,
          "refreshOn": "selectedReportType",
          "validate": {
            "required": true
          },
          "key": "reportName",
          "type": "select",
          "input": true,
          "lockKey": true,
          "autofocus": false,
          "placeholder": "",
          "data": {
            "values": [],
