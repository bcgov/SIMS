          "input": true,
          "components": [
            {
              "input": true,
              "tableView": true,
              "label": "Program Year",
              "key": "programYear",
              "placeholder": "Select Program Year",
              "validate": {
                "required": true
              },
              "type": "select",
              "lockKey": true,
              "customConditional": "show =\r\n  data.reportName === \"Offering_Details_Report\" ||\r\n  data.reportName === \"Student_Unmet_Need_Report\" ||\r\n  data.reportName === \"COE_Requests\";",
              "autofocus": false,
              "data": {
                "values": [
