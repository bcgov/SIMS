          "tree": true,
          "protected": false,
          "persistent": true,
          "clearOnHide": true
        },
        {
          "label": "institutionNames",
          "persistent": false,
          "key": "institutionNames",
          "type": "hidden",
          "input": true,
          "tableView": false,
          "lockKey": true,
          "protected": false,
          "unique": false
        }
      ],
      "clearOnHide": false,
      "theme": "default"
