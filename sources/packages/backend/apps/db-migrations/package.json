{
  "name": "db-migrations",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "migration:run": "node --abort-on-uncaught-exception --require ts-node/register src/main.ts",
    "migration:create": "cd src/migrations && npx typeorm-ts-node-commonjs migration:create",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/data-source.ts"
  },
  "dependencies": {
    "@nestjs/typeorm": "^9.0.0",
    "typeorm": "^0.3.14"
  }
}