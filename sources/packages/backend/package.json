{
  "name": "studentaid-backend",
  "version": "0.0.1",
  "description": "",
  "author": "StudentAid BC",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run migration:run && nest start --watch --preserveWatchOutput",
    "start:debug": "cross-env DEBUG=true nest start --debug --watch --preserveWatchOutput",
    "start:prod:api": "node --enable-source-maps dist/apps/api/main.js",
    "docker:start:api": "node --enable-source-maps dist/apps/api/main.js",
    "start:prod:workers": "node --enable-source-maps dist/apps/workers/main.js",
    "docker:start:workers": "node --enable-source-maps dist/apps/workers/main.js",
    "start:prod:queue-consumers": "node --enable-source-maps dist/apps/queue-consumers/main.js",
    "docker:start:queue-consumers": "node --enable-source-maps dist/apps/queue-consumers/main.js",
    "start:prod:load-test-gateway": "node --enable-source-maps dist/apps/load-test-gateway/main.js",
    "docker:start:load-test-gateway": "node --enable-source-maps dist/apps/load-test-gateway/main.js",
    "test": "cross-env ENVIRONMENT=test jest --verbose --forceExit --config ./jest-unit-tests.json",
    "test:watch": "jest --watch --config ./jest-unit-tests.json",
    "test:cov": "cross-env ENVIRONMENT=test jest --coverage --forceExit --config ./jest-unit-tests.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./jest-unit-tests.json",
    "test:e2e:api:seed": "npm run db:seed:test filter=CreateInstitutionsAndAuthenticationUsers,CreateAESTUsers,CreateStudentUsers,CreateRestrictions,CreateCASDistributionAccounts",
    "test:e2e:api": "npm run test:e2e:api:seed && cross-env ENVIRONMENT=test jest --collect-coverage --verbose --config ./apps/api/test/jest-e2e.json --forceExit",
    "test:e2e:api:local": "cross-env ENVIRONMENT=test TZ=UTC jest --config ./apps/api/test/jest-e2e.json --forceExit",
    "test:e2e:workers": "npm run migration:run && cross-env ENVIRONMENT=test jest --collect-coverage --verbose --config ./apps/workers/test/jest-e2e.json --forceExit",
    "test:e2e:workers:local": "cross-env ENVIRONMENT=test jest --collect-coverage --verbose --config ./apps/workers/test/jest-e2e.json --forceExit",
    "test:e2e:workflow": "npm run deploy:camunda:definitions && cross-env ENVIRONMENT=test jest --verbose --config ./workflow/test/jest-e2e.json --runInBand --forceExit",
    "test:e2e:workflow:local": "cross-env ENVIRONMENT=test TZ=UTC jest --verbose --config ./workflow/test/jest-e2e.json --runInBand --forceExit",
    "test:e2e:queue-consumers:seed": "npm run db:seed:test filter=CreateCASDistributionAccounts",
    "test:e2e:queue-consumers": "npm run test:e2e:queue-consumers:seed && cross-env ENVIRONMENT=test jest --collect-coverage --verbose --config ./apps/queue-consumers/test/jest-e2e.json --runInBand --forceExit",
    "test:e2e:queue-consumers:local": "cross-env ENVIRONMENT=test TZ=UTC jest --verbose --config ./apps/queue-consumers/test/jest-e2e.json --runInBand --forceExit",
    "migration": "ts-node -r tsconfig-paths/register --transpile-only apps/db-migrations/src/main.ts",
    "migration:run": "npm run migration run",
    "migration:list": "npm run migration list",
    "migration:revert": "npm run migration revert",
    "migration:repl": "npm run migration repl",
    "migration:create": "cd apps/db-migrations/src/migrations && npx typeorm-ts-node-commonjs migration:create",
    "db:seed:test": "npm run build test-db-seeding && npm run migration:run && node dist/apps/test-db-seeding/main.js",
    "//db:seed:test:clean": "Drops the database and all its data. Be careful with this command on production since this command will erase all your database tables and their data.",
    "db:seed:test:clean": "npm run build test-db-seeding && node dist/apps/test-db-seeding/main.js task=clean-db",
    "deploy:camunda:definitions": "ts-node ./workflow/src/deploy.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@bull-board/api": "^6.13.0",
    "@bull-board/express": "^6.13.0",
    "@bull-board/nestjs": "^6.13.0",
    "@bull-board/ui": "^6.13.0",
    "@camunda8/sdk": "^8.7.30",
    "@golevelup/nestjs-discovery": "^5.0.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bull": "^11.0.3",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/ssh2-sftp-client": "^9.0.5",
    "adm-zip": "^0.5.16",
    "axios": "^1.12.2",
    "bull": "^4.16.5",
    "clamscan": "^2.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "cron-parser": "^5.4.0",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "express-basic-auth": "^1.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.0",
    "jsonata": "^2.1.0",
    "papaparse": "^5.5.3",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "qs": "^6.14.0",
    "redis": "^5.8.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "soap": "^1.4.2",
    "ssh2-sftp-client": "^12.0.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.901.0",
    "@faker-js/faker": "^10.0.0",
    "@golevelup/ts-jest": "^0.7.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.8",
    "@nestjs/testing": "^11.1.6",
    "@suites/di.nestjs": "^3.0.1",
    "@suites/doubles.jest": "^3.0.1",
    "@suites/unit": "^3.0.1",
    "@types/adm-zip": "^0.5.7",
    "@types/clamscan": "^2.4.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/find-config": "^1.0.4",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.6.2",
    "@types/papaparse": "^5.3.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "find-config": "^1.0.0",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "mockdate": "^3.0.5",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "webpack": "^5.102.0"
  }
}
