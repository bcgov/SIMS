{
  "compilerOptions": {
    "strict": true,
    "baseUrl": "../node_modules",
    "target": "es6",
    "lib": ["es6", "dom"],                       
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true, 
    "emitDecoratorMetadata": true,                
    "experimentalDecorators": true,
    "types": ["cypress","node"],
    "outDir": "tsConvertedFiles",
    
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "tsConvertedFiles",
    "pageObj"
  ]
}