{
  "scripts": {
    "testStudent": "cypress run --browser chrome --spec cypress/integration/01-student/*.spec.ts",
    "testInstitution": "cypress run --browser chrome --spec cypress/integration/02-institution/*.spec.ts",
    "testMinistry": "cypress run --browser chrome --spec cypress/integration/03-ministry/*.spec.ts",
    "Run cypress headless mode command in student": "npm run testStudent",
    "Run cypress headless mode command in institution": "npm run testInstitution",
    "Run cypress headless mode command in ministry": "npm run testMinistry",
    "Run cypress headless mode": "npx cypress run",
    "Run cypress headed mode": "npx cypress open "
  },
  "devDependencies": {
    "cypress": "^9.6.1",
    "cypress-mochawesome-reporter": "^2.3.0",
    "cypress-xpath": "^1.6.2",
    "ts-loader": "^9.2.7",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0"
  }
}
